{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug FastAPI with Uvicorn",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--host",
        "127.0.0.1",
        "--port",
        "8111"
      ],
      "jinja": true,
      "justMyCode": true,
      "env": {
        "PYTHONUNBUFFERED": "1"
      },
      "console": "integratedTerminal"
    }
  ]
}
